function divisivel(numero){
    if(numero % 3 === 0  && numero % 5 === 0 ){
        return 'FizzBuzz'
    }
    if(numero % 3 !== 0 && numero % 5 !== 0){
        return numero
    }
    if(numero % 3 === 0){
        return 'Fizz'
    }
    if(numero % 5 === 0){
        return 'buzz'
    }
    if( typeof(numero) !== 'number'){
        return numero
    }
}

for(let i = 0; i <= 100; i++){
    console.log(i, divisivel(i))
}
